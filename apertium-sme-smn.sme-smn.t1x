    <def-macro n="inherenttag-px-2Param" npar="2">  <!-- Son geahččá birra. Sun kiäččá pirrâsis. -->
	<choose>
	  <when>
	    <test>
	      <equal>
		<clip pos="1" side="tl" part="a_person"/>
		<lit-tag v="p1"/>
	      </equal>
	    </test>
            <let>
              <clip pos="2" side="tl" part="tags"/>
	      <concat>
		<clip pos="2" side="tl" part="tags"/>
		<lit-tag v="px1sg"/>
	      </concat>
            </let>
	  </when>
	  <when>
	    <test><equal><clip pos="1" side="tl" part="a_person"/><lit-tag v="p2"/></equal></test>
            <let>
              <clip pos="2" side="tl" part="tags"/><concat><clip pos="2" side="tl" part="tags"/><lit-tag v="px2sg"/></concat>
            </let>
	  </when>
	  <when>

    <rule comment="REGLA: birra-loc"> <!-- Sii hupmet nieidda birra. Sij sárnuh nieidâst. -->
      <pattern>
        <pattern-item n="nom-gen"/>
        <pattern-item n="birra-po"/>
      </pattern>
      <action>
        <call-macro n="general-macro-2Param">
           <with-param pos="1"/><with-param pos="2"/></call-macro>
        <choose>
          <when>
            <test>
		<in caseless="yes"><var n="current_verb"/><list n="loc_verbs" /></in>
            </test>
            <let><clip pos="2" side="tl" part="whole"/><lit v=""/></let>
            <let><clip pos="1" side="tl" part="a_case"/><lit-tag v="loc"/></let>
          </when>
          <when>
            <test>
              <in caseless="yes"><var n="current_verb"/><list n="acc_verbs" /></in>
            </test>
            <let><clip pos="2" side="tl" part="whole"/><lit v=""/></let>
            <let><clip pos="1" side="tl" part="a_case"/><lit-tag v="acc"/></let>
          </when>
        </choose>
        <call-macro n="remove-semtag-1Param"><with-param pos="1"/></call-macro>
        <out>
           <chunk name="noun" case="caseFirstword">
            <tags>
              <tag><lit-tag v="NP"/></tag>
              <tag><var n="func"/></tag>
              <tag><clip pos="1" side="tl" part="a_case"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/>
              <clip pos="1" side="tl" part="tags"/>
            </lu>
            <b/>
            <lu>
              <clip pos="2" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>




    


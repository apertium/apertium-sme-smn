    <def-macro n="remove-semtag-1Posit" npar="1">
          <let>
            <clip pos="1" side="tl" part="a_sem"/>
            <lit v=""/>
          </let>
    </def-macro>
    <def-macro n="remove-advtag1" npar="1"> <!--  +Adv+Comp +Adv+Superl to +Adv -->
          <let>
            <clip pos="1" side="tl" part="a_comp-superl"/>
            <lit v=""/>

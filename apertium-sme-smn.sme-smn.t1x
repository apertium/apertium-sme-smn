        <pattern-item n="n-sg-accgen"/>                   
      </pattern>     
      <action>
        <let><clip pos="1" side="tl" part="x_func"/><lit v=""/></let> 
        <let><clip pos="1" side="tl" part="a_func"/><lit v=""/></let> 
      <choose><when>
              <test><not><in><clip pos="1" side="sl" part="lem"/><list n="gennum"/></in></not></test> 
        <let><clip pos="2" side="tl" part="tags"/>
        <concat><lit-tag v="n"/><clip pos="2" side="tl" part="a_ntype"/><lit-tag v="par"/></concat></let> 
              </when>
         <otherwise>
        <let><clip pos="2" side="tl" part="a_sem"/><lit v=""/></let>
        <let><clip pos="2" side="tl" part="x_func"/><lit v=""/></let> 
        <let><clip pos="2" side="tl" part="a_func"/><lit v=""/></let> 
         </otherwise>
		</choose>        
		<out>
          <chunk name="num-noun">
            <tags>
              <tag><lit-tag v="SN"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/>    
              <clip pos="1" side="tl" part="tags"/>   
	        </lu>
	        <b/>
	        <lu>
              <clip pos="2" side="tl" part="whole"/>   
		    </lu>

    <rule comment="REGLA: nominal = Add NP to nominal">
      <pattern>
        <pattern-item n="nominal"/>
      </pattern>
      <action>
        <call-macro n="general-macro-1Param"><with-param pos="1"/></call-macro>
        <out>
          <chunk name="nominal" case="caseFirstword">
            <tags>
              <tag><lit-tag v="NP"/></tag>
              <tag><var n="func"/></tag>
              <tag><clip pos="1" side="tl" part="a_PoS"/></tag>
              <tag><clip pos="1" side="tl" part="a_case"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>


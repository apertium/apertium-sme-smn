    <def-macro n="set_caseFirstword" npar="1"
               c="Set the typographic case to be carried by the chunk.
                  - if argument is _not_ a proper noun: caseFirstword is the case of the lemma, and the lemma gets lowercase.
                  - if argument _is_ a proper noun: caseFirstword is lowercase, and lemma stays unchanged.

                  We treat UPPERCASE as proper nouns.">
      <let><var n="caseFirstword"/><lit v="aa"/></let>
      <choose><when>
        <test><not><or>
          <begins-with><clip pos="1" side="tl" part="a_nom"/><lit-tag v="np"/></begins-with>
          <!-- <equal><clip pos="1" side="tl" part="lettercase"/><lit-tag v="fixedcase"/></equal> -->
          <equal><get-case-from pos="1"><lit v="aa"/></get-case-from><lit v="AA"/></equal>
        </or></not></test>
        <let><var n="caseFirstword"/><get-case-from pos="1"><lit v="aa"/></get-case-from></let>
        <modify-case><clip pos="1" side="tl" part="lemh"/><lit v="aa"/></modify-case>
      </when></choose>
    </def-macro>

    <def-macro n="remove-semtag-1Param" npar="1">
          <let>
            <clip pos="1" side="tl" part="a_sem"/>
            <lit v=""/>
          </let>
    </def-macro>

    <def-macro n="remove-advtag-1Param" npar="1"> <!--  +Adv+Comp +Adv+Superl to +Adv -->
          <let>
            <clip pos="1" side="tl" part="a_comp-sup"/><lit v=""/>
          </let>
    </def-macro>

    <def-macro n="agreement-attr-2Param" npar="2"> <!-- pl prn, num, ord , full agreement, exc com -->
       <choose>
         <when>        <!-- with pl.com -->
           <test><or>
            <and>
              <equal><clip pos="2" side="tl" part="a_case"/><lit-tag v="com"/></equal>
              <equal><clip pos="2" side="tl" part="a_nbr"/><lit-tag v="pl"/></equal>
            </and>
            <and>
              <equal><clip pos="2" side="tl" part="a_case"/><lit-tag v="loc"/></equal>
              <equal><clip pos="2" side="tl" part="a_nbr"/><lit-tag v="sg"/></equal>
            </and>
            <and>
              <equal><clip pos="2" side="tl" part="a_case"/><lit-tag v="ill"/></equal>
              <equal><clip pos="2" side="tl" part="a_nbr"/><lit-tag v="sg"/></equal>
            </and>
         </or></test>
         <let>
            <clip pos="2" side="tl" part="tags"/>
            <concat>
              <lit-tag v="n"/>
              <clip pos="2" side="tl" part="a_ntype"/>
              <clip pos="2" side="tl" part="a_nbr"/>
              <clip pos="2" side="tl" part="a_case"/>
            </concat>
        </let>
        <let>
          <clip pos="1" side="tl" part="tags"/>
           <concat>
             <clip pos="1" side="tl" part="a_npagree_type"/>
             <clip pos="1" side="tl" part="a_comp-sup"/>
             <clip pos="2" side="tl" part="a_nbr"/>
             <lit-tag v="gen"/>
             <clip pos="1" side="tl" part="a_foc"/>
           </concat>
        </let>
        </when>
        <otherwise>
                <!-- default -->
           <let>
             <clip pos="2" side="tl" part="tags"/>
             <concat>
                <lit-tag v="n"/>
                <clip pos="2" side="tl" part="a_ntype"/>
                <clip pos="2" side="tl" part="a_nbr"/>
                <clip pos="2" side="tl" part="a_case"/>
              </concat>
           </let>
           <let>
             <clip pos="1" side="tl" part="tags"/>
             <concat>
               <clip pos="1" side="tl" part="a_npagree_type"/>
               <clip pos="1" side="tl" part="a_comp-sup"/>
               <clip pos="2" side="tl" part="a_nbr"/>
               <clip pos="2" side="tl" part="a_case"/>

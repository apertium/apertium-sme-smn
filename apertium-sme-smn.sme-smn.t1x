    <def-macro n="agreement-attr-2Param" npar="2"> <!-- pl prn, num, ord , full agreement, exc com -->
       <choose>
          <when>        <!-- with pl.com -->
            <test>
            <or>
             <and>
        		<equal>
        		  <clip pos="2" side="tl" part="a_cas"/>
        		  <lit-tag v="com"/>
        		</equal>
                        <equal>
        		  <clip pos="2" side="tl" part="a_nbr"/>
        		  <lit-tag v="pl"/>
        		</equal>
              </and>
              <and>
        		<equal>
        		  <clip pos="2" side="tl" part="a_cas"/>
        		  <lit-tag v="loc"/>
        		</equal>
                        <equal>
        		  <clip pos="2" side="tl" part="a_nbr"/>
        		  <lit-tag v="sg"/>
        		</equal>
               </and>
              <and>
        		<equal>
        		  <clip pos="2" side="tl" part="a_cas"/>
        		  <lit-tag v="ill"/>
        		</equal>
                        <equal>
        		  <clip pos="2" side="tl" part="a_nbr"/>
        		  <lit-tag v="sg"/>
        		</equal>
             </and>
	      </or>
	    </test>
         <let>
            <clip pos="2" side="tl" part="tags"/>
    	    <concat>
    	      <lit-tag v="n"/>
    	      <clip pos="2" side="tl" part="a_ntype"/>
    	      <clip pos="2" side="tl" part="a_nbr"/>
    	      <clip pos="2" side="tl" part="a_cas"/>
    	    </concat>
        </let>
        <let>
          <clip pos="1" side="tl" part="tags"/>
	  <concat>
        <clip pos="1" side="tl" part="a_npagree_type"/>
        <clip pos="1" side="tl" part="a_comp-superl"/>
	    <clip pos="2" side="tl" part="a_nbr"/>
	    <lit-tag v="gen"/>
	    <clip pos="1" side="tl" part="a_foc"/>
	  </concat>
        </let>
        </when>
        <otherwise>
                <!-- default -->
           <let>
          	  <clip pos="2" side="tl" part="tags"/>
        	  <concat>
                <lit-tag v="n"/>
   	            <clip pos="2" side="tl" part="a_ntype"/>
	            <clip pos="2" side="tl" part="a_nbr"/>
   	            <clip pos="2" side="tl" part="a_cas"/>
   	          </concat>
           </let>
           <let>
             <clip pos="1" side="tl" part="tags"/>
   	  <concat>
           <clip pos="1" side="tl" part="a_npagree_type"/>
           <clip pos="1" side="tl" part="a_comp-superl"/>
   	    <clip pos="2" side="tl" part="a_nbr"/>
   	    <clip pos="2" side="tl" part="a_cas"/>
   	  </concat>
           </let>
        </otherwise>
        </choose>
    </def-macro>





    <def-macro n="convert-deriv-1Param" npar="1">
      <choose>
        <when>
          <test>
               <equal><clip pos="1" side="sl" part="a_deriv"/><lit-tag v="der_lágan"/></equal>
          </test>
          <let><clip pos="1" side="tl" part="a_deriv"/><lit-tag v="der_lagan"/></let>
        </when>
         <when>
          <test>
               <equal><clip pos="1" side="sl" part="a_deriv"/><lit-tag v="der_passs"/></equal>
          </test>
          <let><clip pos="1" side="tl" part="a_deriv"/><lit-tag v="der_pass"/></let>
          <let><clip pos="1" side="tl" part="a_trans"/><lit v=""/></let>
        </when>
         <when>
          <test>
               <equal><clip pos="1" side="sl" part="a_deriv"/><lit-tag v="der_passl"/></equal>
          </test>
          <let><clip pos="1" side="tl" part="a_deriv"/><lit-tag v="der_pass"/></let>
          <let><clip pos="1" side="tl" part="a_trans"/><lit v=""/></let>
        </when>
         <when>
          <test>
               <equal><clip pos="1" side="sl" part="a_deriv"/><lit-tag v="der_h"/></equal>
          </test>
          <let><clip pos="1" side="tl" part="a_deriv"/><lit-tag v="der_tt"/></let>
          <let><clip pos="1" side="tl" part="a_trans"/><lit v=""/></let>
        </when>
    </choose>

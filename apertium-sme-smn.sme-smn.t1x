    <rule comment="REGLA: birra-loc"> <!-- Sii hupmet nieidda birra. Sij sárnuh nieidâst. -->
      <pattern>
        <pattern-item n="nom-gen"/>
        <pattern-item n="birra-po"/>
      </pattern>
      <action>
        <let><clip pos="1" side="tl" part="x_func"/><lit v=""/></let> 
        <let><clip pos="1" side="tl" part="a_func"/><lit v=""/></let> 
        <let><clip pos="2" side="tl" part="x_func"/><lit v=""/></let> 
        <let><clip pos="2" side="tl" part="a_func"/><lit v=""/></let> 
      <choose>
        <when>
          <test>
               <in caseless="yes"><var n="current_verb"/><list n="loc_verbs" /></in>
          </test>
          <let><clip pos="2" side="tl" part="whole"/><lit v=""/></let> 
          <let><clip pos="1" side="tl" part="a_cas"/><lit-tag v="loc"/></let> 
        </when>
      </choose>
        <call-macro n="remove-semtag-1Posit"><with-param pos="1"/></call-macro>
        <out>
          <chunk name="pp-birra">
            <tags>
              <tag><lit-tag v="SP"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/>    
              <clip pos="1" side="tl" part="tags"/>   
	        </lu>
	        <b/>
            <lu>
              <clip pos="2" side="tl" part="whole"/>    
	        </lu>
          </chunk>
        </out>
      </action>
    </rule>

